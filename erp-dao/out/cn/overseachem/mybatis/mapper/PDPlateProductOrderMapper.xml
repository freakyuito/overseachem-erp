<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 写Sql语句   -->
<mapper namespace="cn.overseachem.mybatis.mapper.PDPlateProductOrderMapper">
    <select id="getOrdersAmountInOneYear" resultType="Integer">
        SELECT COUNT(*) FROM PD_Plate_productOrder WHERE generate_time BETWEEN #{yearBegin} AND #{yearEnd};
    </select>

    <insert id="insertOrder" parameterType="PDPlateProductOrder">
		insert into PD_Plate_productOrder (machine_number,generate_time,maker_name,fk_purchaseOrder_number,productCompany_name)
		values (#{machine_number},#{generate_time},#{maker_name},#{fk_purchaseOrder_number},#{productCompany_name});
	</insert>

</mapper>